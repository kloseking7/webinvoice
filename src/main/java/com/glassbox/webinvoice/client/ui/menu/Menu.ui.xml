<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		#menu-wrapper {
		overflow: hidden;
		height: 52px;
		margin-bottom: 2em;
		background: #3C342E;
		}

		#menu {
		overflow: hidden;
		height: 52px;
		}

		#menu ul {
		margin: 0;
		padding: 0px 0px 0px 0px;
		list-style: none;
		line-height: normal;
		text-align: center;
		}

		#menu li {
		display: inline-block;
		}

		#menu a {
		display: block;
		padding: 0px 30px;
		line-height: 52px;
		border: none;
		text-decoration: none;
		text-transform: lowercase;
		text-align: center;
		text-transform: uppercase;
		font-family: 'Oswald', sans-serif;
		font-size: 14px;
		font-weight: 300;
		color: #FFFFFF;
		}

		#menu a:hover, #menu .current_page_item a {
		background: #DE5635;
		text-decoration: none;
		color: #FFFFFF;
		}

		#menu .current_page_item a {
		}
	</ui:style>
	<g:HTMLPanel>
		<div id="menu-wrapper">
			<div id="menu" class="container">
				<ul>
					<li class="current_page_item">
                                            <g:HTMLPanel ui:field="Home"></g:HTMLPanel>
					</li>
					<li>
                                            <g:HTMLPanel ui:field="Services"></g:HTMLPanel>
					</li>
					<li>
                                            <g:HTMLPanel ui:field="AboutUs"></g:HTMLPanel>
					</li>
					<li>
                                            <g:HTMLPanel ui:field="ContactUs"></g:HTMLPanel>
					</li>
					<li>
                                            <g:HTMLPanel ui:field="Login"></g:HTMLPanel>
					</li>
				</ul>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 